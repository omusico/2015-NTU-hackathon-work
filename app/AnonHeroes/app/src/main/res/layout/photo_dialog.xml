<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/location_photo"
        android:layout_marginTop="@dimen/dialog_margin_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal" />


    <TextView
        android:id="@+id/result"
        android:layout_marginTop="@dimen/dialog_margin_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dialog_margin_top"
        android:layout_marginBottom="@dimen/dialog_margin_top">
        <ImageView
            android:id="@+id/icon110"
            android:visibility="gone"
            android:layout_marginRight="@dimen/dialog_margin_right"
            android:layout_width="@dimen/dialog_type_icon"
            android:layout_height="@dimen/dialog_type_icon"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/dialog_margin_top"/>
        <ImageView
            android:id="@+id/icon119"
            android:visibility="gone"
            android:layout_marginRight="@dimen/dialog_margin_right"
            android:layout_width="@dimen/dialog_type_icon"
            android:layout_height="@dimen/dialog_type_icon"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/dialog_margin_top"/>
        <ImageView
            android:id="@+id/icon113"
            android:visibility="gone"
            android:layout_width="@dimen/dialog_type_icon"
            android:layout_height="@dimen/dialog_type_icon"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/dialog_margin_top"/>
    </LinearLayout>


</LinearLayout>